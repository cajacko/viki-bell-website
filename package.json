{
  "name": "viki-bell-website",
  "version": "1.5.0",
  "description": "Website for Viki Bell",
  "main": "index.js",
  "scripts": {
    "start":
      "concurrently \"npm run dev:webpack\" \"npm run dev:server\" \"npm run dev:serve\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "login": "firebase logout && firebase login",
    "init": "firebase init",
    "deploy:live": "babel-node scripts/setBuild --live && npm run deploy",
    "deploy:dev": "babel-node scripts/setBuild --dev && npm run deploy",
    "deploy": "npm run dev:build && firebase deploy",
    "dev:serve": "firebase serve",
    "dev:server": "babel-node ./devServer/index.js",
    "dev:webpack": "webpack-dev-server",
    "dev:build":
      "webpack --progress --colors --env.production && npm run dev:copyStatic && npm run dev:pages",
    "dev:pages": "babel-node ./scripts/buildPages.jsx",
    "dev:migrate": "babel-node ./scripts/transformPosts.js",
    "dev:sitemap": "babel-node ./scripts/buildSitemap.js",
    "dev:copyStatic": "copyfiles -f src/styles/*.css dist/assets/styles",
    "browsersync": "browser-sync start --proxy http://localhost:5000",
    "postinstall": "yarn dev:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cajacko/viki-bell-website.git"
  },
