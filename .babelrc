{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-flow-strip-types",
    ["module-resolver", {
      "root": ["./web"],
      "alias": {
        "components": "./src/components",
        "actions": "./src/actions",
        "containers": "./src/containers",
        "constants": "./src/constants",
        "helpers": "./src/helpers",
        "views": "./src",
        "store": "./src/store",
        "reducers": "./src/reducers",
        "entry": "./src/entry",
        "dist": "./dist",
        "dev": "./devServer"
      },
      "extensions": [".js", ".jsx"]
      }
    ]
  ]
}
