{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    "relay",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "components": "./src/views/components",
        "containers": "./src/views/containers",
        "constants": "./src/constants",
        "models": "./src/server/models",
        "server": "./src/server",
        "graph": "./src/server/graph",
        "helpers": "./src/helpers",
        "routes": "./src/views/routes",
        "store": "./src/views/store",
        "reducers": "./src/views/reducers",
        "coverage": "./coverage/"
      },
      "extensions": [".js", ".jsx"]
      }
    ]
  ]
}
